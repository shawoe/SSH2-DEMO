<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="member">

    <class name="Friend" table="tab_friend">
    	<!-- 设置主键 -->
        <id name="friendID" column="friend_id" type="java.lang.Integer">
            <generator class="native"/>
        </id>
        
 		<!-- 设置其他属性 -->
        <property name="friendUser" column="friend_user" type="java.lang.String" length="45" />
        <property name="friendTarget" column="friend_target" type="java.lang.String" length="45" />
        <property name="friendTime" column="friendTime" type="java.lang.String" length="45" />
        <property name="friendMutual" column="friend_mutual" type="java.lang.Boolean" />
        
    </class>
    
</hibernate-mapping>
